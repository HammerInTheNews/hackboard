<h1>New Post</h1>

<%= form_for @post, html: {multipart: true} do |f| %>
   
   <div>
      <%= f.file_field :avatar %>
   </div>

   <div>
	   <%= f.label :description %>
	   <%= f.text_area :description %>
   </div>

   <div>
	   <%= f.label :link %>
	   <%= f.text_field :link %>
   </div>


   <%= f.submit %>
<% end %>